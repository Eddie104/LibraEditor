<UserControl x:Class="LibraEditor.egret.resourceTool.ResourceGroup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LibraEditor.egret.resourceTool"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Name="this">
    <Expander Name="headerExpander">
        <ListBox Name="listBox" ItemsSource="{Binding Path=ResourceList,ElementName=this}"/>
    </Expander>
</UserControl>
