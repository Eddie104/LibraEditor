<UserControl x:Class="LibraEditor.mapEditor.view.mapLayer.LayerItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LibraEditor.mapEditor.view.mapLayer"
             mc:Ignorable="d" 
             d:DesignHeight="24" d:DesignWidth="126">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="30"/>
        </Grid.ColumnDefinitions>
        <Label Name="nameLabel" Grid.Column="0"/>
        <CheckBox Grid.Column="1" IsChecked="True" Checked="OnVisibleChanged"/>
    </Grid>
</UserControl>
